<title>System Analysis Tool</title>
<HTA:APPLICATION 
ID="SystemAnalysis"
APPLICATIONNAME="Securedoc Checklist Tool"
BORDER="normal"
BORDERSTYLE="thin"
CAPTION="yes"
CONTEXTMENU="yes"
ICON="Icons\check.ico"
INNERBORDER="no"
MAXIMIZEBUTTON="no"
MINIMIZEBUTTON="yes"
NAVIGABLE="no"
SCROLL="no"
